{
    "name": "sigrun/ceidg-api",
    "description": "PHP CEIDG API library",
    "type": "library",
    "require": {
        "php": ">=7.2",
        "ext-soap": "^7.2",
        "ext-SimpleXML": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "phpunit/phpcov": "^6.0",
        "php-coveralls/php-coveralls": "^2.2",
        "phpunit/php-code-coverage": "^7.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Marek Ognicki",
            "email": "marek@sigrun.eu"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload":{
      "psr-4": {
        "CeidgApi\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "CeidgApi\\Tests\\": "tests/"
      }
    }
}
